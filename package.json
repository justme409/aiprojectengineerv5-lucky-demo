{
  "name": "projectpro-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "services/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --filter web dev",
    "web-dev": "pnpm --filter web dev",
    "build": "pnpm --filter web build",
    "migrate": "pnpm --filter web db:migrate",
    "test-db": "node test-setup.js",
    "check": "pnpm --filter web exec tsc --noEmit ; pnpm --filter web lint",
    "db:up": "bash scripts/db-up.sh",
    "db:ping": "bash scripts/db-ping.sh",
    "web:dev": "bash scripts/web-dev.sh"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "pg": "^8.12.0"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3"
  }
}
